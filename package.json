{
  "name": "coreact-yade",
  "displayName": "coreact-yade",
  "description": "Support for the coreact-yade editor",
  "version": "0.0.8",
  "publisher": "amblafont",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amblafont/vscode-coreact-yade"
  },
  "activationEvents": [
    "onLanguage:coq"
  ],
  "browser": "./dist/web/extension.js",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "coreact-yade.completeDiagram",
        "title": "Complete diagram"
      },
      {
        "command": "coreact-yade.launchYade",
        "title": "Launch YADE"
      },
      {
        "command": "coreact-yade.launchExperimentalYade",
        "title": "Launch experimental YADE"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile-web": "npm run check-types && npm run lint && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch-web": "npm-run-all -p watch-web:*",
    "watch-web:esbuild": "node esbuild.js --watch",
    "watch-web:tsc": "tsc --noEmit --watch --project tsconfig.json"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/assert": "^1.5.10",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.1",
    "@types/vscode": "^1.92.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-web": "^0.0.55",
    "assert": "^2.1.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "glob": "^10.4.2",
    "mocha": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver-types": "^3.17.5",
    "vsls": "^1.0.4753",
    "ws": "^8.18.0"
  }
}
